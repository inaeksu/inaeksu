html, body, div, span, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, img, ol, ul, li,
fieldset, form, label,
table, tr, th, td, button,input,select,textarea {
	background: transparent;
	border: 0;
	margin: 0;
	padding: 0;
	vertical-align: baseline;
}

/************ START ****************/
body { 
	background: #FCFCFC; 
	font-family: Arial, Helvetica, sans-serif; 
	font-size: 12px; 
	color: #666; 
}

a { 
	color: #666; 
	text-decoration: none; 
	outline: none; 
}

a:hover { 
	text-decoration: underline; 
}

h1 { 
	font-size: 28px; 
}

h2 { 
	font-size: 24px; 
}

h3 { 
	font-size: 20px; 
}

h4 { 
	font-size: 16px; 
}

/***** HEADER *****/
.header { 
	width: 100%; 
	margin: 0; 
	height: 55px; 
	margin-bottom: 10px; 
	background: #f7f7f7; 
	border-bottom: 1px solid #ccc; 
}

.headerinner {
	padding: 8px; 
	position: relative; 
	width: 960px; 
	margin: auto; 
}

.logo { 
	margin-top: 2px; 
}

.notiPanel { 
	padding: 12px 10px;  
	float: left; 
	position: relative; 
	border-right: 1px solid #ccc; 
}

.userPanel {
	padding: 20px 10px;  
	float: left; 
	position: relative; 
	border-right: 0; 
}

.headright { 
	position: absolute; 
	top: 0; 
	right: 0; 
}

.notialert { 
	display: inline-block; 
	margin-top: 0px;
	padding: 8px 10px 8px 10px; 
	background: #cc0000; 
	color: #fff; 
	font-weight: bold;	
	border-radius: 2px; 
}

.userinfo { 
	padding: 7px 10px 7px 10px; 
	border-radius: 2px; 
	border: 1px solid #ccc; 
	font-weight: bold; 
	color: #666; 
	background: #fcfcfc; 
}

.userinfo:hover { 
	background-color: #eee; 
	text-decoration: none; 
}

/***** MAIN *****/
.mainwrapper { 
	width: 960px; 
	margin: auto; 
	position: relative; 
}

.maincontent { 
	margin: 0 0px 0 210px; 
}

.maintabmenu { 
	list-style: none; 
	margin: 0; 
	line-height: 21px; 
	position: relative; 
	z-index: 5; 
}

.maintabmenu li { 
	display: inline-block; 
}

.maintabmenu li a { 
	padding: 5px 20px 5px 20px; 
	color: #666; 
	font-family: 'BebasNeueRegular', Arial, Helvetica, sans-serif; 
	font-size: 13px; 
	font-weight: bold;
	-moz-border-radius: 2px 2px 0 0; 
	-webkit-border-radius: 2px 2px 0 0; 
	border-radius: 2px 2px 0 0; 
	border: 1px solid #ddd;
	letter-spacing: 0.8px; 
	display: block; 
	background: #eee url(../images/titlebg.png) repeat-x top left; 
	text-shadow: 1px 1px #f7f7f7;
}

.content { 
	border: 1px solid #ddd; 
	background: #fff; 
	margin-top: -1px; 
	line-height: 21px; 
	border-radius: 0px 2px 2px 2px;
}

.main{ 
	position:relative; 
	float:left; 
	width: 728px; 
	padding: 10px; 
	line-height: 21px; 
}

.mborder{ 
	border-top: 1px solid #ddd; 
}

/********** LEFT PANEL *******************/
.mainleft { 
	width: 200px;
	position: absolute; 
	top: 0; 
	left: 0; 
	padding-bottom: 20px; 
}

.leftmenu ul { 
	list-style: none;
}

.leftmenu ul li { 
	display: block; 
	position: relative; 
}

.leftmenu ul li a { 
	border: 1px solid #ddd; 
	border-top: 0; 
	display: block; 
	background: #fcfcfc url(sprites.png); 
	color: #666; 
	padding-left: 34px;
	background-repeat: no-repeat; 
	background-position: 8px center; 
	font-weight: bold;
}

.leftmenu ul li a:hover {  
	text-decoration: none; 
	background-color: #eee; 
}

.leftmenu ul li:first-child a { 
	border-top: 1px solid #ddd; 
	-moz-border-radius: 2px 2px 0 0; 
	-webkit-border-radius: 2px 2px 0 0; 
	border-radius: 2px 2px 0 0; 
}

.leftmenu ul li:last-child a { 
	-moz-border-radius: 0 0 2px 2px; 
	-webkit-border-radius: 0 0 2px 2px; 
	border-radius: 0 0 2px 2px; 
}

.leftmenu ul li a span { 
	display: block; 
	padding: 8px 10px; 
	border-left: 1px solid #eee; 
	background: #fff; 
}

.leftmenu ul li a:hover span { 
	border-left: 1px solid #ddd; 
	background: #f7f7f7; 
}

.leftmenu ul li a em {
 	font-style: normal; 
}

.leftmenu ul li a.widgets { 
	background-position: -190px -443px; 
}

/********** BUTTONS ********************/
.buttonlist { 
	list-style: none; 
	height: 31px; 
	padding: 10px;
}

.buttonlist li { 
	display: inline-block; 
	margin-bottom: 15px; 
	margin-right: 7px; 
}

.stdbtn { 
	font-weight: bold; 
	padding: 7px 10px; 
	border: 1px solid #ccc; 
	background: #fcfcfc; 
	color: #666; 
	-moz-border-radius: 2px; 
	-webkit-border-radius: 2px; 
	border-radius: 2px; 
	cursor: pointer;
}

.stdbtn:hover { 
	text-decoration: none; 
	background-color: #eee;
}

.stdbtn:active { 
	background: #eee; 
	-webkit-box-shadow: inset 0 3px 5px rgba(0, 0, 0, 0.125); 
	box-shadow: inset 0 3px 5px rgba(0, 0, 0, 0.125); 
}

input[type="submit"] {
	width: 115px; 
	font-weight: bold; padding: 7px 10px; border: 1px solid #ccc; background: #fcfcfc; color: #666; 
	-moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius: 2px; cursor: pointer;
}

input[type="submit"]:hover { 
	text-decoration: none; 
	background-color: #eee;
}

input[type="submit"]:active { 
	background: #eee; 
	-webkit-box-shadow: inset 0 3px 5px rgba(0, 0, 0, 0.125); 
	box-shadow: inset 0 3px 5px rgba(0, 0, 0, 0.125); 
}

input[type="text"],input[type="password"] {
	width: 200px;
	padding: 4px;
}

select {
	width: 210px;
	padding: 3px 3px 3px 0px;
}

input, select {
	border: 1px solid #cccccc;
}

.row {
	margin-bottom: 5px;
}

.errorSummary {
	padding: 5px;
	border: 1px solid #cc0000;
}

.errorSummary li {
	color: #cc0000;
	list-style: none;
}

.errorMessage {
	color: #cc0000;
}

#statistic {
	margin-bottom: 3px;
}

#statistic td {
	max-width: 35px;
}

#statistic input[type="text"] {
	width: 25px;
	text-align: center;
	padding: 4px;
}

.columbs {
	border-top: 1px solid #666;
}